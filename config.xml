<?xml version='1.0' encoding='utf-8'?>
<widget id="com.harmonicnw.techstart.fhsapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>FHSApp</name>
    <description>
        FHS App
    </description>
    <author email="harmonicnw@gmail.com" href="http://www.harmonicnw.com">
        Dustin Diep, Julian Weisbord, Griffin Drake, Joshua Hauser
    </author>
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    
    <!-- PLUGINS -->
    <feature name="http://api.phonegap.com/1.0/file"/>
    
    <!-- SPLASH SCREENS -->
    <gap:splash src="splash.png" />

    <!-- iOS -->
    <gap:splash src="Images/app_splash/ios/Default~iphone.png" gap:platform="ios" width="320" height="480" />
	<gap:splash src="Images/app_splash/ios/Default@2x~iphone.png" gap:platform="ios" width="640" height="960" />
	<gap:splash src="Images/app_splash/ios/Default-568h@2x~iphone.png" gap:platform="ios" width="640" height="1136" />
	<gap:splash src="Images/app_splash/ios/Default-Landscape~ipad.png" gap:platform="ios" width="1024" height="748" />
	<gap:splash src="Images/app_splash/ios/Default-Portrait~ipad.png" gap:platform="ios" width="768" height="1004" />
	<!-- retina iPad support: PhoneGap 2.5.0+ only -->
	<gap:splash src="Images/app_splash/ios/Default-Landscape@2x~ipad.png" gap:platform="ios" width="2048" height="1496" />
	<gap:splash src="Images/app_splash/ios/Default-Portrait@2x~ipad.png" gap:platform="ios" width="1536" height="2008" />
	
	<!-- ICONS -->
	<icon src="icon.png" />
	
	<!-- iOS -->
	<icon src="Images/app_icon/ios/icon.png" gap:platform="ios" width="57" height="57" />
	<icon src="Images/app_icon/ios/icon-72.png" gap:platform="ios" width="72" height="72" />
	<icon src="Images/app_icon/ios/icon@2x.png" gap:platform="ios" width="114" height="114" />
	<!-- retina iPad support: PhoneGap 2.5.0+ only -->
	<icon src="Images/app_icon/ios/icon-72@2x.png" gap:platform="ios" width="144" height="144" />
	
</widget>
